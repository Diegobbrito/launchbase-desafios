{% block content %}
<div class="item">
<h3>Email</h3>
<input 
 type="email" 
 name="email" 
 placeholder="Email" 
 value="{{user.email}}"
 onblur="Validate.apply(this, 'isEmail')"
 required
>
</div>
<h3>Senha</h3>
<input type="password" name="password" placeholder="Digite sua senha" required>
{% endblock content %}
